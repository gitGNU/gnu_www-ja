" penpos7(1.5thick,80); penpos8(1.3thick,90); penpos9(1.5thick,800);\n"
" x8=.5w;\n"
" z7r=z1r; z9l=z6l; y8=(.5-bigfuzz)*h;\n"
" penstroke z7e..z8e..z9e;\n"
" penlabels(1,2,3,4,5,6,7,8,9);\n"
"endchar;\n"
"\n"
"end;\n"
