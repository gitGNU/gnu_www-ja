msgstr "<a href=\"http://retout.co.uk/\">Tim Retout</a>"

# type: Content of: <p>
#. type: Content of: <p>
msgid "is the maintainer of <a href=\"/software/enscript/\">GNU Enscript</a>."
msgstr "es quien mantiene <a href=\"/software/enscript/\">Enscript de GNU</a>."

# type: Content of: <h4>
#. type: Content of: <h4>
