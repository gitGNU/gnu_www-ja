msgid ""
"<a href=\"http://planet.gnu.org/\">Planet GNU</a> <a href=\"http://planet."
"gnu.org/rss20.xml\">"
msgstr ""
"<a href=\"http://planet.gnu.org/\">Planet GNU</a> <a href=\"http://planet."
"gnu.org/rss20.xml\">"

