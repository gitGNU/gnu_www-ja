msgid "<a href=\"http://member.fsf.org\">Associate Membership</a>"
msgstr "<a href=\"http://member.fsf.org\">Mitgliedschaft als Partner</a>"

#. type: Content of: <ul><li>
msgid "<a href=\"http://patron.fsf.org\">Corporate Patronage</a>"
