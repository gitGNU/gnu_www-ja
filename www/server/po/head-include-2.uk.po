"   line-height: 1.4em;\n"
"}\n"
"#fsf-support, #fundraiser, .progress {\n"
"   display: none;\n"
"}\n"
"p#fundraiser {\n"
"   float: right; \n"
"   font-size: 1.1em;\n"
"   margin: 0 0 1.2em 2em;\n"
"}\n"
